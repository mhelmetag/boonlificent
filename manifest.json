{
  "manifest_version": 2,
  "name": "Boonlificent",
  "short_name": "Boonlificent",
  "author": "Max Helmetag",
  "version": "1.0.1",
  "version_name": "1.0.1",
  "description": "Auto payroll deduction helper for Boonli",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.boonli.com/checkout"
      ],
      "js": [
        "build/content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
