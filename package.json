{
  "name": "boonlificent",
  "version": "1.0.0",
  "description": "Auto payroll deduction helper for Boonli",
  "main": "src/content.js",
  "scripts": {
    "clean": "rm -rf build && rm -f boonlificent.zip",
    "build": "./node_modules/.bin/babel src -d build",
    "zip": "zip boonlificent.zip manifest.json build/*",
    "release": "npm run clean && npm run build && npm run zip"
  },
  "repository": {
    "type": "git",
    "url": "github.com/mhelmetag/boonlificent"
  },
  "keywords": [
    "boonli",
    "hax"
  ],
  "author": "Max Helmetag",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "eslint": "^5.12.1",
    "eslint-config-google": "^0.11.0"
  }
}
